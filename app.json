{
  "expo": {
    "name": "emall-frontend",
    "slug": "emall-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/logoM.png",
      "resizeMode": "contain",
      "height": "150",
      "width": "150",
      "backgroundColor": "#E74A5F"
    },
